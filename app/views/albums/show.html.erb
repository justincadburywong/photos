<div id="fh5co-main">
  <p id="notice"><%= notice %></p>

  <div class="fh5co-gallery popup-gallery ">
    <% @album.photos.each do |photo| %>
      <a class="gallery-item" href="<%= url_for(photo) %>">
        <%= image_tag photo %>
      </a>
    <% end %>
  </div>

  <%= link_to 'Edit', edit_album_path(@album) %> |
  <%= link_to 'Back', albums_path %>

</div>
